            		Command="{Binding ButtonGoHomeMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Home}" Style="{DynamicResource sButton_Home}" SnapsToDevicePixels="True" TabIndex="20"/>
                <Button Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Button_Prev}" d:LayoutOverrides="Height"
            		Command="{Binding ButtonGoPreMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Prev}" SnapsToDevicePixels="True" Style="{DynamicResource sButton_Prev}" TabIndex="30"/>
                <Button Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Button_Next}" d:LayoutOverrides="Height"
            		Command="{Binding ButtonGoNextMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Next}" SnapsToDevicePixels="True" Style="{DynamicResource sButton_Next}" TabIndex="40"/>
            </StackPanel>
            
            <StackPanel Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right">
                <Button x:Name="xBackwardMoveButton" Content="-" Margin="3,5" Width="30" Height="25" HorizontalAlignment="Right" Command="{Binding MoveBackwardPlaybackCommand}" Style="{DynamicResource sButton_PrevTime}"/>
                <Grid x:Name="xDateGrid" Grid.Row="0" Margin="0,8" Background="Transparent" Width="160">
                	<Rectangle x:Name="rectangle"  Fill="#FF333333" Stroke="#FF444444" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" RadiusX="2" RadiusY="2"  Margin="0" Grid.RowSpan="2"  />
                	<StackPanel Margin="0" Orientation="Horizontal" d:LayoutOverrides="Height" HorizontalAlignment="Center" VerticalAlignment="Center">
                		<TextBlock x:Name="xDateTextBlock" Margin="0,0,5,0" TextWrapping="Wrap" Foreground="#FFE4E4E4" HorizontalAlignment="Center"
                			Text="{Binding PlaybackDateText}" FontSize="13" VerticalAlignment="Center" />
                		<TextBlock x:Name="xTimeTextBlock" TextWrapping="Wrap" Text="{Binding PlaybackTimeText}" Margin="0" 
                			Foreground="#FFE4E4E4" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="13"/>
                	</StackPanel>
                	<Popup x:Name="xTimePickerControlPopup" AllowsTransparency="False" Grid.Row="0" Grid.Column="0" Visibility="Visible" 
                		StaysOpen="False" Placement="Top">
                		<ui:TimePickerControl x:Name="xTimePickerControl"/>
                	</Popup>
                </Grid>
                <Button x:Name="xForwardMoveButton" Content="+" Margin="3,5" Width="30" Height="25" HorizontalAlignment="Right" Command="{Binding MoveForwardPlaybackCommand}" Style="{DynamicResource sButton_NextTime}"/>
                <TextBlock TextWrapping="Wrap" Text="Interval :" VerticalAlignment="Center" Margin="5,0,3,0" Foreground="White"/>
                <timerPickerControls:TimePicker x:Name="xIntervalTimePicker" HorizontalAlignment="Stretch" Width="100" Height="25"
                                                SelectedHour="{Binding DefaultIntervalHours, UpdateSourceTrigger=PropertyChanged}"
                                                SelectedMinute="{Binding DefaultIntervalMinutes, UpdateSourceTrigger=PropertyChanged}"
                                                SelectedSecond="{Binding DefaultIntervalSeconds, UpdateSourceTrigger=PropertyChanged}"
                                                Style="{DynamicResource sTimePicker}" Margin="0"/>
                <Grid Width="2" Margin="10">
                	<Rectangle Fill="Black" StrokeThickness="0" Width="1" HorizontalAlignment="Left"/>
                	<Rectangle Fill="#FF595959" StrokeThickness="0" Width="1" HorizontalAlignment="Right"/>
                </Grid>
                <Button x:Name="xPlayButton" Content="Play" Margin="0,5" Width="40" Height="30" HorizontalAlignment="Right" Command="{Binding PlayPlaybackCommand}" Style="{DynamicResource sButton_Play}"/>
                <Button x:Name="xPauseButton" Content="Pause" Margin="3,5,0,5" Width="40" Height="30" HorizontalAlignment="Right" Command="{Binding PausePlaybackCommand}" Style="{DynamicResource sButton_Pause}"/>
                <Grid Width="2" Margin="10,10,5,10">
                	<Rectangle Fill="Black" StrokeThickness="0" Width="1" HorizontalAlignment="Left"/>
                	<Rectangle Fill="#FF595959" StrokeThickness="0" Width="1" HorizontalAlignment="Right"/>
                </Grid>
                <Button x:Name="xEndPlaybackButton" Margin="5" Width="80" Height="30" HorizontalAlignment="Right" Command="{Binding EndPlaybackCommand}" Style="{DynamicResource sButton_MapPlaybackOn}" Content="PLAYBACK"/>
                <Button
                	x:Name="xAddMemoButton"
                	Content="MEMO"
                	Command="{Binding AddMemoCommand}" Style="{DynamicResource sButton_Memo}" Width="30" Height="30" Margin="0,0,10,0" ToolTip="Post-it"
                    Visibility="{Binding IsReportWritingMode, Converter={StaticResource BooleanToVisibilityConverter}}"
                    />
            </StackPanel>
        </Grid>
        <Grid x:Name="xGridPrivateToolBar" Grid.Row="1" Height="40" Visibility="{Binding PrivateResourceToolbarVisibility}">
        	<Grid.ColumnDefinitions>
        		<ColumnDefinition Width="auto"/>
        		<ColumnDefinition Width="*"/>
        		<ColumnDefinition Width="auto"/>
        	</Grid.ColumnDefinitions>
            <Border Background="#A5000000" CornerRadius="3" BorderBrush="Silver" BorderThickness="1" Grid.ColumnSpan="3"/>
            <StackPanel Margin="0" Orientation="Horizontal" d:LayoutOverrides="Width" Grid.Column="1" VerticalAlignment="Center">
                <Button Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Button_Home}" d:LayoutOverrides="Height"
            		Command="{Binding ButtonGoHomeMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Home}" Style="{DynamicResource sButton_Home}" SnapsToDevicePixels="True" TabIndex="20"/>
                <Button Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Button_Prev}" d:LayoutOverrides="Height"
            		Command="{Binding ButtonGoPreMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Prev}" SnapsToDevicePixels="True" Style="{DynamicResource sButton_Prev}" TabIndex="30"/>
                <Button Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Button_Next}" d:LayoutOverrides="Height"
            		Command="{Binding ButtonGoNextMapCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Next}" SnapsToDevicePixels="True" Style="{DynamicResource sButton_Next}" TabIndex="40"/>
                <ToggleButton x:Name="xCameraOn" Height="30" Width="30" Content="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Label_VideoVisibilityToggle}" d:LayoutOverrides="Height"
                    IsChecked="{Binding DoHideAllCamera, UpdateSourceTrigger=PropertyChanged}" 
                    Visibility="{Binding IsEnabledControlCameraObject, Converter={StaticResource BooleanToVisibilityConverter}, UpdateSourceTrigger=PropertyChanged}"
                    IsEnabled="{Binding IsEnabledControlCameraObject, UpdateSourceTrigger=PropertyChanged}"
                    Margin="5,0,0,0" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tootip_VideoVisibilityToggle}" SnapsToDevicePixels="True" Style="{DynamicResource sToggleButton_CameraHide}" TabIndex="50"/>
                <TextBlock TextWrapping="Wrap" Text="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Label_SearchCamera}" VerticalAlignment="Center" Margin="10,0,5,0" Foreground="#FF999999"
                               Visibility="{Binding IsEnabledControlCameraObject, Converter={StaticResource BooleanToVisibilityConverter}, UpdateSourceTrigger=PropertyChanged}"/>
                <ComboBox x:Name="xComboCameraList" Width="120" Height="20"
                               Visibility="{Binding IsEnabledControlCameraObject, Converter={StaticResource BooleanToVisibilityConverter}, UpdateSourceTrigger=PropertyChanged}"
                               SelectedValue="{Binding SelectedCameraData, UpdateSourceTrigger=PropertyChanged}"
                               IsEnabled="{Binding IsEnabledControlCameraObject, UpdateSourceTrigger=PropertyChanged}"  
                               DisplayMemberPath="Name" Grid.Column="1"
                               ItemsSource="{Binding CameraList, UpdateSourceTrigger=PropertyChanged}"
                               Foreground="White"
                               Style="{DynamicResource sComboBoxPresetSequence}" ItemContainerStyle="{DynamicResource sComboBoxItem_Common}" TabIndex="10"/>
            </StackPanel>
            <StackPanel Margin="0,0,10,0" Orientation="Horizontal" Grid.Column="2" d:LayoutOverrides="Width">
                <ToggleButton x:Name="xImageLocation" Height="30" Width="30" Style="{DynamicResource sToggleButton_Location_VW}"
                	Margin="5,0,0,0" IsChecked="{Binding DoMakeLocation, UpdateSourceTrigger=PropertyChanged}" 
                              Command="{Binding ButtonMakeLocationCommand, UpdateSourceTrigger=PropertyChanged}" 
                              SnapsToDevicePixels="True" ToolTip="{x:Static Language:Resource_ArcGISControls_Tools_VWToolControl.Tooltip_Location}" TabIndex="60"/>
                <Button x:Name="xImageBookmark" Content="BOOKMARK" Width="80" Height="30" Style="{DynamicResource sButton_Bookmark_VW}" 
                        Command="{Binding ButtonMakeBookMarkCommand, UpdateSourceTrigger=PropertyChanged}" Margin="5,0,0,0" TabIndex="80"/>
                <Button x:Name="xStartPlaybackButton" Content="PLAYBACK" Margin="5,0,0,0" Width="80" Height="30" Command="{Binding StartPlaybackCommand}" Style="{DynamicResource sButton_MapPlayback}" TabIndex="90"/>
                <Button
                	x:Name="xAddMemoButton1"
                	Content="MEMO"
                	Command="{Binding AddMemoCommand}" Style="{DynamicResource sButton_Memo}" Width="30" Height="30" Margin="5,0,0,0" TabIndex="70" ToolTip="Post-it"
                    Visibility="{Binding IsReportWritingMode, Converter={StaticResource BooleanToVisibilityConverter}}"
                    />
            </StackPanel>
        </Grid>
    </Grid>
</UserControl>
