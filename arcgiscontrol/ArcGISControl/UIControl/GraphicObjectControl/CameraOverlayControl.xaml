<UserControl x:Class="ArcGISControl.UIControl.GraphicObjectControl.CameraOverlayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:GraphicObjectControl="clr-namespace:ArcGISControl.UIControl.GraphicObjectControl" 
             mc:Ignorable="d" Background="Transparent" x:Name="xRoot">
    <Grid>
        <GraphicObjectControl:PanomorphOverlayUserControl x:Name="xPanomorphOverlayControl"></GraphicObjectControl:PanomorphOverlayUserControl>
        <GraphicObjectControl:IndependentPlaybackOverlayControl x:Name="xIndependentPlaybackOverlayControl" Margin="5" Background="Transparent"/>
        <GraphicObjectControl:RdsOverlayControl x:Name="xRdsOverlayControl" Background="{x:Null}"/>
    </Grid>
</UserControl>
